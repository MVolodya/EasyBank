@model EasyBank.Models.Currency

@{
    ViewBag.Title = @Resources.Resource.CurrencyDelete;
}

<h2>@Html.Label(@Resources.Resource.CurrencyDelete)</h2>
<br />
@Html.ActionLink(@Resources.Resource.BackToList, "CurrencyList")
<br /><br />

<h3>@Resources.Resource.AskToDeleteCurrency</h3>
<fieldset>
    <legend>@Resources.Resource.Currency</legend>

    <div class="display-label">
        @Html.DisplayNameFor(model => model.CurrencyName)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CurrencyName)
    </div>
</fieldset>
@using (Html.BeginForm("CurrencyDelete", "Protected"))
{
    <p>
        @Html.Hidden("id", Model.CurrencyId)
        <input type="submit" value=@Resources.Resource.Delete />
    </p>
}
